        <td>
            @Html.DisplayFor(modelItem => item.HproductslistNAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HproductslistDESC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Hproductslistcompanyid)
